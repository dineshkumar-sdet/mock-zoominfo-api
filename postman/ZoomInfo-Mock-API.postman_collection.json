      ]
    },
    {
      "name": "4. Bulk Operations",
      "item": [
        {
          "name": "Submit Bulk Search Job",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('job_id', jsonData.jobId);",
                  "    console.log('Job ID saved:', jsonData.jobId);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"jobName\": \"Software Companies Search\",\n  \"searchCriteria\": {\n    \"industry\": \"Software & Technology\",\n    \"employeesMin\": 1000\n  },\n  \"maxResults\": 50000\n}"
            },
            "url": {
              "raw": "{{base_url}}/search/company/bulk",
              "host": ["{{base_url}}"],
              "path": ["search", "company", "bulk"]
            },
            "description": "Submit a bulk company search job"
          },
          "response": []
        },
        {
          "name": "Get Bulk Job Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/search/company/bulk/{{job_id}}",
              "host": ["{{base_url}}"],
              "path": ["search", "company", "bulk", "{{job_id}}"]
            },
            "description": "Check the status of a bulk search job"
          },
          "response": []
        }
      ]
    },
